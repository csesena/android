<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
     <item android:state_pressed="true"
           android:drawable="@drawable/repeat_pressed" /> <!-- pressed -->
     <item android:state_focused="true"
           android:drawable="@drawable/repeat_focused" /> <!-- focused -->
     <item android:state_enabled="false"
           android:drawable="@drawable/repeat_desv" /> <!-- default disabled -->
     <item android:state_enabled="true"
           android:drawable="@drawable/repeat_normal" /> <!-- default enabled -->
 </selector>